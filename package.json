{
	"name": "nice-vscode-extension",
	"displayName": "nice_vscode_extension",
	"description": "my first vscode extension",
	"publisher": "wangyjhh",
	"version": "0.0.2",
	"icon": "resource/icon.png",
	"repository": {
		"url": "https://gitee.com/DVN/translation_vscode_extension.git"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "wyj.translationCommand",
				"title": "Translate"
			},
			{
				"command": "wyj.toUpperCaseCommand",
				"title": "ToUpperCase"
			}
		],
		"keybindings": [
			{
				"command": "wyj.translationCommand",
				"key": "alt+w",
				"mac": "alt+w"
			},
			{
				"command": "wyj.toUpperCaseCommand",
				"key": "alt+q",
				"mac": "alt+q"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "pnpm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "pnpm run compile && pnpm run lint",
		"lint": "eslint src --ext ts",
		"test": "vitest --run"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.9.3",
		"vitest": "^0.27.1"
	},
	"dependencies": {
		"axios": "^1.2.2"
	}
}
